{
  "extends": "astro/tsconfigs/strictest",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "unplugin-icons/types/react",
      "vite-plugin-pwa/client",
      "vite-plugin-pwa/info"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
